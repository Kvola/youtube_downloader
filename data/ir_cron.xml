<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron : Réinitialiser les téléchargements Telegram orphelins -->
        <record id="ir_cron_reset_stuck_telegram_downloads" model="ir.cron">
            <field name="name">YouTube Downloader : Réinitialiser téléchargements Telegram bloqués</field>
            <field name="model_id" ref="model_telegram_channel_video"/>
            <field name="state">code</field>
            <field name="code">model._cron_reset_stuck_downloads()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="priority">100</field>
        </record>
    </data>
</odoo>
